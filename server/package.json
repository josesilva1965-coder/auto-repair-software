{
  "name": "auto-repair-shop-server",
  "version": "1.0.0",
  "description": "Backend for the auto repair shop application",
  "main": "dist/src/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/src/index.js",
